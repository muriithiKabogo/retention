
<div class="col-lg-12 col-sm-12 col-xs-12">
  <div class="white-box">
    <div class="table-responsive" style="min-height: 500px;max-height: 1000px;overflow-y: auto;">
      <% unless @result.blank? %>
        <table class="table table-bordered ">
          <thead>
            <tr class="bg-info">
              <%@thindex = @result.first.keys.index("_user")%>
              <%@result.first.keys.rotate(@thindex).each do |key|%>
                <th><%=key%></th>
                    <%@all_keys.push(key)%>
                  <%end%>
            </tr>            
          </thead>
                         
          <tbody style="font-size: 12px;">
          <%@result.each do |row|%> 
              <tr>
                <%@all_keys.each do |key|%>
                  <td>
                    <%=row[key]%>
                  </td> 
                <%end%>  
              </tr>
           <%end%>  
          </tbody>
        </table>
      <% else %>
        <p>There is no result that matches your search!</p>
      <% end %>
    </div>
  </div>
</div>