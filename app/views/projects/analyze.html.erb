<%= render 'layouts/header' %>
<div id="wrapper">
  <%= render 'layouts/navbar' %>
  <div id="page-wrapper">
      <div class="container-fluid">
        <div class="row bg-title">
            <div class="col-lg-3 col-md-4 col-sm-4 col-xs-12">
         <h4 class="page-title">Analyze Data</h4>
            </div>
            <div class="col-lg-9 col-sm-8 col-md-8 col-xs-12">
                <!-- <a href="#" target="_blank" class="btn btn-info pull-right m-l-20 hidden-xs hidden-sm waves-effect waves-light">DEMO</a>
                 -->
                <!-- <a href="/dashboard/emptystate" type="button" class="btn btn-info pull-right">
                empty state
                </a> -->
            </div>
            <!-- /.col-lg-12 -->
        </div>
        <% if flash[:notice] && @user.sign_in_count<=1%>
                  <div class="alert alert-success" role="alert">
                      <%= flash[:notice] %>
                  </div>
        <% end %>
        <div class="col-md-12">
          <div class="col-md-2">Select Event</div>
          <div class="col-md-10"><%= select_tag :event, options_for_select(@events, @events.first), id: "selectEvent" %></div>
        </div>
        <div class="col-md-12">
          <div class="col-md-2">Filters</div>
          <div class="col-md-10">
            <div id="query-builder"></div>
          </div>
        </div>
        <div class="col-md-12">
          <div class="col-md-2"></div>
          <div class="col-md-10">
            <div class="view"><button class="btn btn-info search-sql">Search</button></div>
          </div>
        </div>

        <div class="col-md-12">
          <div class="col-md-2">Result</div>
          <div class="col-md-10"></div>
        </div>
        <div id="search-result"></div>
      </div>
      <!-- /.container-fluid -->
      <%= render 'layouts/footer' %>
  </div>
<!-- /#page-wrapper -->
</div>
<%= javascript_include_tag "application-builder" %>
